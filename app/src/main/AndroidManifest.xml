<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.avi.workoutmanagement" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".CreateLog"
            android:label="@string/title_activity_create_log" >
        </activity>
        <activity
            android:name=".WeightsActivity"
            android:label="@string/title_activity_weights" >
        </activity>
        <activity
            android:name=".WorkoutHistory"
            android:label="@string/title_activity_workout_history" >
        </activity>
    </application>

</manifest>
